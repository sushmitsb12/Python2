help_dict = {
    'one': '1',
    'two': '2',
    'three': '3',
    'four': '4',
    'five': '5',
    'six': '6',
    'seven': '7',
    'eight': '8',
    'nine': '9',
    'zero' : '0'
}

s1 = input("Enter the string : " )
res = ''.join(help_dict[ele] for ele in s1.split())
print("The string after performing replace : " + res)
